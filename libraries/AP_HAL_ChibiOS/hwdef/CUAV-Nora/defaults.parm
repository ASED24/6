# setup the onboard PWM number
BRD_PWM_COUNT 14

# setup the temperature compensation
BRD_IMU_TARGTEMP -1

# turn on the CAN power monitoring(default)
CAN_P1_DRIVER 1
BATT_MONITOR 8

# turn on SBUS output
SERIAL5_PROTOCOL 15

# setup the parameter for the ADC power module
BATT_VOLT_PIN 16
BATT_CURR_PIN 17
BATT_VOLT_MULT 18.000
BATT_AMP_PERVLT 24.000

